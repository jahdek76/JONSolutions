{
  "name": "rice-caller-ai",
  "version": "1.0.0",
  "description": "AI-powered calling system for rice milling company",
  "main": "backend/app.js",
  "scripts": {
    "start": "node backend/app.js",
    "dev": "nodemon backend/app.js",
    "client": "cd frontend && npm start",
    "dev:full": "concurrently \"npm run dev\" \"npm run client\"",
    "build": "cd frontend && npm install && CI=false npm run build",
    "vercel-build": "cd frontend && npm install && CI=false npm run build"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "elevenlabs-node": "^1.1.3",
    "express": "^4.18.2",
    "mongoose": "^8.0.1",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.20.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
} 